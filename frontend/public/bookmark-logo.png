    </span><span><span class="hljs-title function_">expect</span></span><span>(res.</span><span><span class="hljs-property">body</span></span><span>.</span><span><span class="hljs-property">title</span></span><span>).</span><span><span class="hljs-title function_">toBeDefined</span></span><span>()
  })

  </span><span><span class="hljs-title function_">it</span></span><span>(</span><span><span class="hljs-string">'should get bookmarks'</span></span><span>, </span><span><span class="hljs-keyword">async</span></span><span> () =&gt; {
    </span><span><span class="hljs-keyword">const</span></span><span> res = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">request</span></span><span>(app)
