        {summary.length &gt; 300 &amp;&amp; (
          &lt;button
            onClick={() =&gt; setExpanded(!expanded)}
            className="block mt-2 text-blue-500 text-xs underline"
          &gt;
            {expanded ? "Show less" : "Read more"}
          &lt;/button&gt;
        )}
      &lt;/div&gt;
    &lt;/div&gt;
  );
};

