            target="_blank"
            rel="noreferrer"
            className="text-blue-600 font-semibold"
          &gt;
            {data.title}
          &lt;/a&gt;
        &lt;/div&gt;
        &lt;FaTrash
          onClick={() =&gt; onDelete(data._id)}
          className="text-red-500 cursor-pointer"
        /&gt;
      &lt;/div&gt;

      &lt;div className="text-sm text-gray-700 max-h-48 overflow-y-auto bg-gray-50 p-2 rounded"&gt;
