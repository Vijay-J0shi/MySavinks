      </span><span><span class="hljs-keyword">const</span></span><span> u = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title function_">URL</span></span><span>(url)
      favicon = u.</span><span><span class="hljs-property">origin</span></span><span> + favicon
    }

    </span><span><span class="hljs-keyword">const</span></span><span> summaryRes = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">fetch</span></span><span>(</span><span><span class="hljs-string">`https://r.jina.ai/http://<span class="hljs-subst">${<span class="hljs-built_in">encodeURIComponent</span></span></span></span><span>(url)}`)
    </span><span><span class="hljs-keyword">const</span></span><span> summary = </span><span><span class="hljs-keyword">await</span></span><span> summaryRes.</span><span><span class="hljs-title function_">text</span></span><span>()

    </span><span><span class="hljs-keyword">const</span></span><span> bookmark = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title class_">Bookmark</span></span><span>.</span><span><span class="hljs-title function_">create</span></span><span>({
