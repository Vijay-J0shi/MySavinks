      .</span><span><span class="hljs-title function_">set</span></span><span>(</span><span><span class="hljs-string">'Authorization'</span></span><span>, </span><span><span class="hljs-string">`Bearer <span class="hljs-subst">${token}</span></span></span><span>`)

    </span><span><span class="hljs-title function_">expect</span></span><span>(res.</span><span><span class="hljs-property">statusCode</span></span><span>).</span><span><span class="hljs-title function_">toBe</span></span><span>(</span><span><span class="hljs-number">200</span></span><span>)
    </span><span><span class="hljs-title function_">expect</span></span><span>(</span><span><span class="hljs-title class_">Array</span></span><span>.</span><span><span class="hljs-title function_">isArray</span></span><span>(res.</span><span><span class="hljs-property">body</span></span><span>)).</span><span><span class="hljs-title function_">toBe</span></span><span>(</span><span><span class="hljs-literal">true</span></span><span>)
