</span><span><span class="hljs-keyword">export</span></span><span> </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title function_">addBookmark</span></span><span> = </span><span><span class="hljs-keyword">async</span></span><span> (</span><span><span class="hljs-params">req, res</span></span><span>) =&gt; {
  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> { url } = req.</span><span><span class="hljs-property">body</span></span><span>

    </span><span><span class="hljs-keyword">const</span></span><span> response = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">fetch</span></span><span>(url)
    </span><span><span class="hljs-keyword">const</span></span><span> html = </span><span><span class="hljs-keyword">await</span></span><span> response.</span><span><span class="hljs-title function_">text</span></span><span>()
    </span><span><span class="hljs-keyword">const</span></span><span> $ = cheerio.</span><span><span class="hljs-title function_">load</span></span><span>(html)

    </span><span><span class="hljs-keyword">const</span></span><span> title = $(</span><span><span class="hljs-string">'title'</span></span><span>).</span><span><span class="hljs-title function_">text</span></span><span>() || url
    </span><span><span class="hljs-keyword">let</span></span><span> favicon = $(</span><span><span class="hljs-string">'link[rel="icon"]'</span></span><span>).</span><span><span class="hljs-title function_">attr</span></span><span>(</span><span><span class="hljs-string">'href'</span></span><span>) || </span><span><span class="hljs-string">'/favicon.ico'</span></span><span>
