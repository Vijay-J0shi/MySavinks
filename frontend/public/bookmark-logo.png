</span><span><span class="hljs-keyword">import</span></span><span> { setupTestDB, cleanupTestDB } </span><span><span class="hljs-keyword">from</span></span><span> </span><span><span class="hljs-string">'./setup.js'</span></span><span>
</span><span><span class="hljs-keyword">import</span></span><span> jwt </span><span><span class="hljs-keyword">from</span></span><span> </span><span><span class="hljs-string">'jsonwebtoken'</span></span><span>

</span><span><span class="hljs-keyword">let</span></span><span> token

</span><span><span class="hljs-title function_">beforeAll</span></span><span>(</span><span><span class="hljs-keyword">async</span></span><span> () =&gt; {
  </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">setupTestDB</span></span><span>()
  </span><span><span class="hljs-keyword">const</span></span><span> user = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title class_">User</span></span><span>.</span><span><span class="hljs-title function_">create</span></span><span>({ </span><span><span class="hljs-attr">email</span></span><span>: </span><span><span class="hljs-string">'book@test.com'</span></span><span>, </span><span><span class="hljs-attr">password</span></span><span>: </span><span><span class="hljs-string">'123456'</span></span><span> })
  token = jwt.</span><span><span class="hljs-title function_">sign</span></span><span>({ </span><span><span class="hljs-attr">id</span></span><span>: user.</span><span><span class="hljs-property">_id</span></span><span> }, process.</span><span><span class="hljs-property">env</span></span><span>.</span><span><span class="hljs-property">JWT_SECRET</span></span><span>)
})

</span><span><span class="hljs-title function_">afterAll</span></span><span>(cleanupTestDB)

</span><span><span class="hljs-title function_">describe</span></span><span>(</span><span><span class="hljs-string">'Bookmark Routes'</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
  </span><span><span class="hljs-title function_">it</span></span><span>(</span><span><span class="hljs-string">'should add a bookmark'</span></span><span>, </span><span><span class="hljs-keyword">async</span></span><span> () =&gt; {
