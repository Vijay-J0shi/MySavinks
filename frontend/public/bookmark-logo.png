  </span><span><span class="hljs-keyword">if</span></span><span> (!user) </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">400</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">'Invalid credentials'</span></span><span> })
  </span><span><span class="hljs-keyword">if</span></span><span> (!(</span><span><span class="hljs-keyword">await</span></span><span> bcrypt.</span><span><span class="hljs-title function_">compare</span></span><span>(password, user.</span><span><span class="hljs-property">password</span></span><span>))) {
    </span><span><span class="hljs-keyword">return</span></span><span> res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">400</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">'Invalid credentials'</span></span><span> })
  }
  </span><span><span class="hljs-keyword">const</span></span><span> token = jwt.</span><span><span class="hljs-title function_">sign</span></span><span>({ </span><span><span class="hljs-attr">id</span></span><span>: user.</span><span><span class="hljs-property">_id</span></span><span> }, process.</span><span><span class="hljs-property">env</span></span><span>.</span><span><span class="hljs-property">JWT_SECRET</span></span><span>, { </span><span><span class="hljs-attr">expiresIn</span></span><span>: </span><span><span class="hljs-string">'7d'</span></span><span> })
  res.</span><span><span class="hljs-title function_">json</span></span><span>({ token })
}
