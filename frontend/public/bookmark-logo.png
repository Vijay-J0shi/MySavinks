import fetch from "node-fetch"
import cheerio from "cheerio"

export const addBookmark = async (req, res) =&gt; {
  try {
    const { url } = req.body

    const response = await fetch(url)
    const html = await response.text()
    const $ = cheerio.load(html)

    const title = $('title').text() || url
    let favicon = $('link[rel="icon"]').attr('href') || '/favicon.ico'
    if (favicon.startsWith('/')) {
      const u = new URL(url)
      favicon = u.origin + favicon
    }

    const summaryRes = await fetch(<code>https://r.jina.ai/http://${encodeURIComponent(url)}</code>)
    const summary = await summaryRes.text()

    const bookmark = await Bookmark.create({
      user: req.userId,
      url,
      title,
      favicon,
      summary,
    })

    res.status(201).json(bookmark)
  } catch (error) {
    res.status(500).json({ message: <code>Add bookmark error: ${error}</code> })
  }
}

export const getBookmarks = async (req, res) =&gt; {
  const bookmarks = await Bookmark.find({ user: req.userId }).sort({ createdAt: -1 })
  res.json(bookmarks)
}

export const deleteBookmark = async (req, res) =&gt; {
  const { id } = req.params
  await Bookmark.deleteOne({ _id: id, user: req.userId })
  res.json({ message: "Bookmark deleted" })
