            maxAge:7 * 24 * 60 * 60 * 1000
        })
        return res.status(201).json(user)

    }catch(error){
        return res.status(500).json({message:"internal server error"})
    }
} 

export const logIn = async(req,res)=&gt;{
    try{
        let {email , password}= req.body
