  } </span><span><span class="hljs-keyword">catch</span></span><span> (error) {
    res.</span><span><span class="hljs-title function_">status</span></span><span>(</span><span><span class="hljs-number">500</span></span><span>).</span><span><span class="hljs-title function_">json</span></span><span>({ </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-string">`Add bookmark error: <span class="hljs-subst">${error}</span></span></span><span>` })
  }
}

</span><span><span class="hljs-keyword">export</span></span><span> </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-title function_">getBookmarks</span></span><span> = </span><span><span class="hljs-keyword">async</span></span><span> (</span><span><span class="hljs-params">req, res</span></span><span>) =&gt; {
